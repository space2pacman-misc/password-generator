{
	"manifest_version": 2,
	"name": "Password generator",
	"description": "Генератор паролей",
	"version": "1.0.0",
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "index.html"
	},
	"content_scripts": [{
    	"matches":    ["*://*/*"],
        "js":         ["js/background.js"]
    }],
	"permissions": ["activeTab"]
}